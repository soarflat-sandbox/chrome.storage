{
  "name": "chrome.storage",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.dmm.co.jp/digital/videoa/-/detail/=/*"
      ],
      "js": [
        "js/item.js"
      ]
    }
  ],
  "options_ui": {
    "page": "history.html",
    "open_in_tab": true
  }
}
